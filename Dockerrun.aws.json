{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "search-web-api",
            "image": "357210185381.dkr.ecr.us-east-1.amazonaws.com/wormbase/search-web-api",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3006,
                    "containerPort": 3000
                }
            ],
	    "links": ["elasticsearch"],
	    "environment": [
		{
		    "name": "ES_BASE_URI",
		    "value": "http://elasticsearch:9200"
		}
	    ]
        },
	{
	    "name": "elasticsearch",
	    "image": "357210185381.dkr.ecr.us-east-1.amazonaws.com/wormbase/aws-elasticsearch:latest",
	    "essential": true,
	    "portMappings": [
		{
		    "hostPort": 9200,
		    "containerPort": 9200
		},
		{
		    "hostPort": 9300,
		    "containerPort": 9300
		}
	    ]
	}
    ]
}
